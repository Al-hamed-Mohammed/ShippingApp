@model IEnumerable<Shipping_Label_App.ViewModel.LabelVM>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}


<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Label</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">All Label</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
<section class="content p-3">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">All Labels</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FromName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.ToName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FromCity)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.ToCity)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FromState)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.ToState)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Provider.ProviderName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Class.ClassName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.TrackingNo)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.ApplicationUser.Name)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FromName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ToName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FromCity)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ToCity)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FromState)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ToState)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ProviderName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ClassName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.TrackingNo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.UserName)
                                    </td>

                                </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

